{
  "name": "webpack-template",
  "version": "0.0.1",
  "description": "a webpack project template",
  "main": "index.js",
  "scripts": {
    "dev:dll": "cross-env NODE_ENV=development webpack --config scripts/webpack.dll.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack.dev.config.js --port 8080 --hot --open",
    "prod:dll": "cross-env NODE_ENV=production webpack -p --config scripts/webpack.dll.config.js",
    "prod": "cross-env NODE_ENV=production webpack -p --config scripts/webpack.prod.config.js",
    "mock": "json-server mock/db.js --routes mock/routes.json --port 9000",
    "build": "npm run prod:dll && npm run prod",
    "test": "cross-env NODE_ENV=test ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnatarHe/webpack-template.git"
  },
  "keywords": [],
  "author": "AnnatarHe<iamhele1994@gamil.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnnatarHe/webpack-template/issues"
  },
  "homepage": "https://github.com/AnnatarHe/webpack-template#readme",
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-test-env.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-do-expressions": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-proposal-function-bind": "^7.10.1",
    "@babel/plugin-proposal-function-sent": "^7.10.1",
    "@babel/plugin-proposal-json-strings": "^7.10.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.1",
    "@babel/plugin-proposal-throw-expressions": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.1",
